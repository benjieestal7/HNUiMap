<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HNUiMap.pages.Account.CreateAccountPage"
             xmlns:pages="clr-namespace:HNUiMap.pages.Account"
             NavigationPage.HasNavigationBar="False">
    
      <MasterDetailPage.Master>
            <pages:CreateAccountPageFlyout x:Name="FlyoutPage" />
      </MasterDetailPage.Master>

      <MasterDetailPage.Detail>
            <NavigationPage>
                <x:Arguments>
                    <pages:CreateAccountPageDetail />
                </x:Arguments>
            </NavigationPage>
      </MasterDetailPage.Detail>  

</MasterDetailPage>